{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "installCommand": "npm install",
  "framework": "nextjs",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    }
  ],
  "env": {
    "MONGODB_URI": "@mongodb_uri",
    "NEXT_PUBLIC_API_URL": "@next_public_api_url"
  }
}